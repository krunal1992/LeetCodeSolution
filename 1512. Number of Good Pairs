


class Solution {
    public int numIdenticalPairs(int[] nums) {
        int pairCount=0;
        for(int index=0;index<nums.length-1;index++){
            for(int j=index+1;j<nums.length;j++){
                if(nums[index] == nums[j]){
                    pairCount++;
                }
            }
        }
        return pairCount;
    }
}


